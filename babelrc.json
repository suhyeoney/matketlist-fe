{
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "@animations": "./src/app/animations",
            "@api": "./src/app/api",
            "@assets": "./src/app/assets",
            "@dataTypes": "./src/app/dataTypes",
            "@floats": "./src/app/floats",
            "@hashtags": "./src/app/hashtags",
            "@hooks": "./src/app/hooks",
            "@main": "./src/app/main",
            "@modals": "./src/app/modals",
            "@services": "./src/app/services",
            "@signIn": "./src/app/signIn",
            "@sliders": "./src/app/sliders",            
            "@spinners": "./src/app/spinners",
            "@store": "./src/app/store",
            "@tables": "./src/app/tables",
            "@utils": "./src/app/utils"
          }
        }
      ]
    ]
  }